doctype html
html(lang="en")
  head
    block title
      title SurveyHide
    block meta
      meta(charset='UTF-8')
      meta(name='viewport', content='width=device-width, initial-scale=1.0')
      meta(name='description' content='Welcome to SurveyHide')
    block link
      link(rel='stylesheet', href='/surveyhide/css/bootstrap.min.css')
      link(rel='stylesheet', href='/surveyhide/css/custom.css')
      link(rel='stylesheet', href='/surveyhide/css/validetta.css', type='text/css', media='screen')
  body
    include header

    main
      .container
        block content

    include scripts
